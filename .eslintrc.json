{
  
  "extends": [
    "prettier"
  ],
  "parser":"@typescript-eslint/parser",
  "parserOptions": {
    "requireConfigFile": false,
    "project": "./tsconfig.json",
    "sourceType": "module",
    "ecmaVersion": 2020,
    "ecmaFeatures": {
    "experimentalObjectRestSpread": true
  },
  "rules": {
     "no-console": "warn",
     "camelcase": "off"
  }
}
}
